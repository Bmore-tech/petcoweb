<mvc:View 
    xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form">

        <form:Form width="100%" editable="true" visible="true">
            <form:title>
                    <core:Title text="Nuevo Concepto" level="Auto"/>
                </form:title>
            <form:formContainers>
                <form:FormContainer visible="true">
                    <form:formElements>
                        <form:FormElement visible="true">
                            <form:label>
                                <Label text="Id" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                            </form:label>
                            <form:fields>
                                <Input type="Text" showValueHelp="false" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                <Input type="Text" showValueHelp="false" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                            </form:fields>
                        </form:FormElement>
                        <form:FormElement visible="true">
                            <form:label>
                                <Label text="Cuenta Contable" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                            </form:label>
                            <form:fields>
                                <Input type="Text" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                            </form:fields>
                        </form:FormElement>
                        <form:FormElement visible="true">
                            <form:label>
                                <Label text="Grupo Aprovador" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                            </form:label>
                            <form:fields>
                                <Input type="Text" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                            </form:fields>
                        </form:FormElement>

                        <form:FormElement visible="true">
                            <form:label>
                                <Label text="Grupo prevalidador" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                            </form:label>
                            <form:fields>
                                <Input type="Text" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                            </form:fields>
                        </form:FormElement>

                        <form:FormElement visible="true">
                            <form:fields>
                                <Button text="Guardar" type="Accept" icon="sap-icon://save" iconFirst="true" width="162px" enabled="true" visible="true" iconDensityAware="false"/>
                                <Button text="Cancelar" type="Reject" icon="sap-icon://cancel" iconFirst="true" width="163px" enabled="true" visible="true" iconDensityAware="false"/>
                            </form:fields>
                        </form:FormElement>
                    </form:formElements>
                    <form:title/>
                </form:FormContainer>
            </form:formContainers>
            <form:layout>
                <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
            </form:layout>
        </form:Form>

</mvc:View>