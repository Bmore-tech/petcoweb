{
  "_version": "1.12.0",
  "sap.app": {
    "id": "com.bmore.portalproveedores",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.bmore.portalproveedores.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.113.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.m": {}
      }
    },
    "handleValidation": true,
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.bmore.portalproveedores.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.bmore.portalproveedores.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true,
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "TargetLogin",
          "target": "TargetLogin"
        },
        {
          "name": "Main",
          "pattern": "Main",
          "target": [
            "main"
          ]
        },
        {
          "name": "Reception",
          "pattern": "Reception",
          "target": [
            "Reception"
          ]
        },
        {
          "name": "Concepts",
          "pattern": "Concepts",
          "target": [
            "Concepts"
          ]
        },
        {
          "name": "Subsidiary",
          "pattern": "Subsidiary",
          "target": [
            "Subsidiary"
          ]
        }
      ],
      "targets": {
        "main": {
          "viewId": "main",
          "viewName": "Main"
        },
        "TargetLogin": {
          "viewType": "XML",
          "viewId": "Login",
          "viewName": "Login"
        },
        "Reception": {
          "viewType": "XML",
          "viewId": "Reception",
          "viewName": "Reception"
        },
        "Concepts":{
          "viewType": "XML",
          "viewId": "Conceps",
          "viewName": "Conceps"
        },
        "Subsidiary":{
          "viewType": "XML",
          "viewId": "Subsidiary",
          "viewName": "Subsidiary"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    }
  }
}
