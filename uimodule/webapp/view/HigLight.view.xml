<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.bmore.inveweb.controller.HighLight" xmlns="sap.m" xmlns:layout="sap.ui.layout" 
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
	displayBlock="true"
	height="100%">
    <Page showHeader="false" title="Responsive Page Title" showFooter="true" showNavButton="false">
        <ScrollContainer height="100%" width="100%" vertical="true">
		<f:GridContainer id="Grid" class="sapUiSmallMargin">
			<f:layout>
				<f:GridContainerSettings rowSize="84px" columnSize="84px" gap="8px" />
			</f:layout>
			<f:layoutXS>
				<f:GridContainerSettings rowSize="70px" columnSize="70px" gap="8px" />
			</f:layoutXS>

            <f:Card>
				<f:layoutData>
					<f:GridContainerItemLayoutData minRows="5" columns="5" />
				</f:layoutData>
				<f:header>
					<card:Header title="Stock Age Index Índice de Vida de Anaquel de PT" press="openChart"/>
				</f:header>
				<f:content>
					<HBox id="myHBox"></HBox>
				</f:content>
			</f:Card>

			


		</f:GridContainer>
	</ScrollContainer>
    </Page>
</mvc:View>
